# SFMC API Configuration
# Copy this file to .env.local and fill in your SFMC credentials

# SFMC OAuth Credentials (from Installed Package in SFMC Setup)
SFMC_CLIENT_ID=your_client_id_here
SFMC_CLIENT_SECRET=your_client_secret_here

# SFMC Subdomain (e.g., if your auth URL is https://abc123.auth.marketingcloudapis.com, use 'abc123')
SFMC_SUBDOMAIN=your_subdomain_here

# SFMC Account ID (also called MID - Marketing Cloud ID)
SFMC_ACCOUNT_ID=your_account_id_here

# Data Extension External Key
DE_EXTERNAL_KEY=D4344287-6DDF-49F6-B7A5-A7E0043A3C2C

# Optional: Node environment
NODE_ENV=production
